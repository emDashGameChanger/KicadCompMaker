(footprint "CP_Radial_D{{ diameter }}mm_P{{ pinPitch }}mm_H{{ height }}mm"
	(version 20240108)
	(generator "EmDashGameChanger's footprint generator, template stolen from the kind folks at KiCad")
	(layer "F.Cu")
	(descr "CP, Radial series, Radial, pin pitch={{ pinPitch }}mm, diameter={{ diameter }}mm, height={{ height }}mm, Electrolytic Capacitor")
	(tags "CP Radial series Radial pin pitch {{ pinPitch }}mm diameter {{ diameter }}mm height {{ height }}mm Electrolytic Capacitor")
	(property "Reference" "REF**"
		(at {{ diameter/2 + 1.5 }} 0 0)
		(layer "F.SilkS")
		(effects
			(font
				(size 1 1)
				(thickness 0.15)
			)
		)
	)
	(property "Value" "CP_Radial_D{{ diameter }}mm_P{{ pinPitch }}mm"
		(at {{ diameter/2 + 1.5 }} 2 0)
		(layer "F.Fab")
		(effects
			(font
				(size 1 1)
				(thickness 0.15)
			)
		)
	)
	(attr through_hole)

	(fp_circle
		(center 0 0)
		(end {{ (diameter/2) + 0.12 }} 0)
		(stroke
			(width .12)
			(type solid)
		)
		(fill no)
		(layer "F.SilkS")
	)
	(fp_circle
		(center 0 0)
		(end {{ (diameter/2) + 0.12 + 0.1 }} 0)
		(stroke
			(width 0.05)
			(type solid)
		)
		(fill no)
		(layer "F.CrtYd")
	)

	(fp_line
		(start -1.133605 -1.0875)
		(end -0.633605 -1.0875)
		(stroke
			(width 0.1)
			(type solid)
		)
		(layer "F.Cu")
	)
	(fp_line
		(start -0.883605 -1.3375)
		(end -0.883605 -0.8375)
		(stroke
			(width 0.1)
			(type solid)
		)
		(layer "F.Cu")
	)

    (fp_line
        (start {{ plus_center_x - 0.25 }} {{ plus_center_y }})
        (end {{ plus_center_x + 0.25 }} {{ plus_center_y }})
        (stroke (width 0.1) (type solid))
        (layer "F.SilkS")
    )
    (fp_line
        (start {{ plus_center_x }} {{ plus_center_y - 0.25 }})
        (end {{ plus_center_x }} {{ plus_center_y + 0.25 }})
        (stroke (width 0.1) (type solid))
        (layer "F.SilkS")
    )

	(fp_circle
		(center 0 0)
		(end {{ diameter/2 }} 0)
		(stroke
			(width 0.1)
			(type solid)
		)
		(fill no)
		(layer "F.Fab")
	)

    {{ poly_red }}
    {{ poly_green_top }}
    {{ poly_green_bottom }}
    {{ poly_blue }}

	(pad "1" thru_hole roundrect
		(at -{{ pinPitch/2 }} 0)
		(size 1.6 1.6)
		(drill 0.8)
		(layers "*.Cu" "*.Mask")
		(roundrect_rratio 0.15625)
	)
	(pad "2" thru_hole circle
		(at {{ pinPitch/2 }} 0)
		(size 1.6 1.6)
		(drill 0.8)
		(layers "*.Cu" "*.Mask")
	)
)